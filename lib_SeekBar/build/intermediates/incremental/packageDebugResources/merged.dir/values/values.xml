<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="def_pointer_color">#FFFFFF</color>
    <color name="def_reached_color">#FFD844</color>
    <color name="def_wheel_color">#4E4988</color>
    <declare-styleable name="CircleSeekBar">
        <attr format="integer" name="wheel_max_process"/>
        <attr format="integer" name="wheel_cur_process"/>
        <attr format="integer" name="wheel_max_start_process"/>
        <attr format="integer" name="wheel_cur_start_process"/>
        <attr format="integer" name="wheel_max_end_process"/>
        <attr format="integer" name="wheel_cur_end_process"/>
        <attr format="integer" name="wheel_gap_process"/>
        <attr format="color" name="wheel_reached_color"/>
        <attr format="color" name="wheel_reached_color_select"/>
        <attr format="color" name="wheel_reached_color_start_normal"/>
        <attr format="color" name="wheel_reached_color_start_select"/>
        <attr format="color" name="wheel_reached_color_end_normal"/>
        <attr format="color" name="wheel_reached_color_end_select"/>

        <attr format="dimension" name="wheel_reached_width"/>
        <attr format="boolean" name="wheel_reached_has_corner_round"/>
        <attr format="color" name="wheel_unreached_color"/>
        <attr format="dimension" name="wheel_unreached_width"/>
        <attr format="color" name="wheel_pointer_color"/>
        <attr format="color" name="wheel_time_color"/>
        <attr format="dimension" name="wheel_pointer_radius"/>
        <attr format="boolean" name="wheel_has_pointer_shadow"/>
        <attr format="boolean" name="wheel_has_wheel_shadow"/>
        <attr format="dimension" name="wheel_pointer_shadow_radius"/>
        <attr format="dimension" name="wheel_shadow_radius"/>
        <attr format="boolean" name="wheel_has_cache"/>
        <attr format="boolean" name="wheel_can_touch"/>
        <attr format="boolean" name="wheel_scroll_only_one_circle"/>
        <attr format="reference" name="wheel_center_background_drawable"/>
        <attr format="integer" name="wheel_start_drawable"/>
        <attr format="reference" name="wheel_end_drawable"/>
        <attr format="reference" name="wheel_center_start_drawable"/>
        <attr format="reference" name="wheel_center_end_drawable"/>
        <attr format="integer" name="wheel_start_drawable_select"/>
        <attr format="reference" name="wheel_end_drawable_select"/>
        <attr format="reference" name="wheel_center_start_drawable_select"/>
        <attr format="reference" name="wheel_center_end_drawable_select"/>
    </declare-styleable>
    <dimen name="def_shadow_offset">2dp</dimen>
    <dimen name="def_shadow_radius">2dp</dimen>
    <dimen name="def_wheel_width">32dp</dimen>
    <string name="app_name">lib</string>
</resources>